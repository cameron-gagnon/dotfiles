{
       "update.mode": "none",
       "gitlens.hovers.currentLine.over": "line",
       "terminal.integrated.defaultProfile.osx": "tmux",
       "terminal.external.osxExec": "iTerm.app",
       "vim.showMarksInGutter": true,
       "vim.leader": ",",
       "vim.vimrc.enable": true,
       "gitlens.remotes": [
        {
          "domain": "git.corp.stripe.com",
          "type": "GitHub"
        }
       ],
       "search.exclude": {
              "sorbet/shims/autogenerated": true
       },
       "workbench.iconTheme": "file-icons",
       "editor.renderWhitespace": "boundary",
       "editor.wordBasedSuggestionsMode": "allDocuments",
       "vim.smartRelativeLine": true,
       "go.toolsManagement.autoUpdate": true,
       "editor.cursorBlinking": "solid",
       "editor.cursorStyle": "line-thin",
       "trailing-spaces.trimOnSave": true,
       "terminal.integrated.fastScrollSensitivity": 0.1,
       "editor.fastScrollSensitivity": 1,
       "workbench.list.fastScrollSensitivity": 1,
       "editor.minimap.enabled": false,
       "files.exclude": {
              "**/tags": true,
              "**/vendor/**": true
       },
       "files.watcherExclude": {
              "**/vendor/**": true
       },
       "terminal.integrated.cursorBlinking": true,
       "terminal.integrated.macOptionClickForcesSelection": true,
       "terminal.integrated.persistentSessionScrollback": 500,
       "terminal.integrated.scrollback": 10000,
       "terminal.integrated.tabs.location": "left",
       "terminal.integrated.tabs.showActiveTerminal": "never",
       "terminal.integrated.windowsEnableConpty": false,
       "terminal.integrated.commandsToSkipShell": [
              "extension.vim_navigateCtrlL"
       ],
       "terminal.integrated.allowChords": false,
       "go.coverOnSingleTest": true,
       "go.autocompleteUnimportedPackages": true,
       "go.editorContextMenuCommands": {
              "fillStruct": true
       },
       "testing.openTesting": "neverOpen",
       "gitlens.codeLens.recentChange.command": "gitlens.revealCommitInView",
       "gitlens.keymap": "alternate",
       "redhat.telemetry.enabled": false,
       "tabnine.experimentalAutoImports": true,
       "bazel.buildifierFixOnFormat": true,
       "bazel.enableCodeLens": true,
       "sorbet.enabled": true,
       "sorbet.lspConfigs": [

              {
                     "id": "stable",
                     "name": "Sorbet",
                     "description": "Stable Sorbet Ruby IDE features",
                     "cwd": "${workspaceFolder}",
                     "command": [
                            "/Users/stroopc/.rbenv/shims/bundler",
                            "exec",
                            "srb",
                            "typecheck",
                            "--lsp"
                     ]
              },
              {
                     "id": "beta",
                     "name": "Sorbet (Beta)",
                     "description": "Beta Sorbet Ruby IDE features",
                     "cwd": "${workspaceFolder}",
                     "command": [
                            "bundle",
                            "exec",
                            "srb",
                            "typecheck",
                            "--lsp",
                            "--enable-all-beta-lsp-features"
                     ]
              },
              {
                     "id": "experimental",
                     "name": "Sorbet (Experimental)",
                     "description": "Experimental Sorbet Ruby IDE features (warning: crashy, for developers only)",
                     "cwd": "${workspaceFolder}",
                     "command": [
                            "bundle",
                            "exec",
                            "srb",
                            "typecheck",
                            "--lsp",
                            "--enable-all-experimental-lsp-features"
                     ]
              }
       ],
       "workbench.colorCustomizations": {
         "terminalCursor.foreground": "#f6f2f2",
         "terminalCursor.background": "#000000"
       },
       "update.channel": "none",
       "security.workspace.trust.enabled": false,
       "telemetry.enableTelemetry": false,
       "remote.autoForwardPorts": false,
       "remote.restoreForwardedPorts": false,
       "remote.SSH.enableAgentForwarding": false,
       "remote.SSH.path": "sc-ssh-wrapper",
       "remote.SSH.useLocalServer": true,
       "debug.javascript.automaticallyTunnelRemoteServer": false,
       "go.survey.prompt": false,
       "workbench.editor.enablePreviewFromCodeNavigation": true,
       "workbench.editor.enablePreviewFromQuickOpen": true,
       "typescript.tsdk": "~/stripe/pay-server/frontend/js-scripts/node_modules/typescript/lib",
       "debug.terminal.clearBeforeReusing": true,
       "telemetry.telemetryLevel": "off",
       "remote.SSH.connectTimeout": 60,
       "protoc": {
              "options": [
                     "--proto_path=${workspaceRoot}/pkg/pandora/proto",
              ]
       },
       "ruby.codeCompletion": "rcodetools",
       "ruby.intellisense": "rubyLocate",
       "window.title": "${appName}${separator}${rootPath}${separator}${activeEditorMedium}",
       "gitlens.views.branches.branches.layout": "list",
       "workbench.colorTheme": "Tomorrow Night Blue",
       "run-pay.liveNudging.enabled": false,
       "editor.inlineSuggest.enabled": true,
       "github.copilot.enable": {
       "*": false,
       "apex": true,
       "erb": true,
       "go": true,
       "java": true,
       "javascript": true,
       "python": true,
       "ruby": true,
       "rust": true,
       "scala": true,
       "shellscript": true,
       "starlark": true,
       "terraform": true,
       "typescript": true,
       "typescriptreact": true
       },
       "search.followSymlinks": false,
       "remote.SSH.configFile": "~/.ssh/devboxes",
       "explorer.confirmDragAndDrop": false,
       "ruby.rubocop.hideDisableSuggestions": true,
       "remote.SSH.enableRemoteCommand": true,
       "git.openRepositoryInParentFolders": "never",
       "editor.formatOnSave": true,
       "editor.defaultFormatter": "hashicorp.terraform",
       "remote.forwardOnOpen": false,
       "remote.otherPortsAttributes": {
       "onAutoForward": "ignore"
       },
       "gitlens.ai.experimental.generateCommitMessage.enabled": false,
       "editor.stickyScroll.enabled": true,
       "settingsSync.ignoredSettings": [],
       "window.zoomLevel": 2,
       "files.insertFinalNewline": true,
       "files.trimTrailingWhitespace": true,
       "files.trimFinalNewlines": true
}
